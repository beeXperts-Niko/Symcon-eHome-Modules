{
    "elements": [
        { "type": "ValidationTextBox", "name": "Address", "caption": "Hostname or IP address" },
        { "type": "NumberSpinner", "name": "RefreshInterval", "caption": "Refresh interval (seconds)", "minimum": 30 }
    ],
    "actions": [
        { "type": "Button", "label": "Receive values", "onClick": "RSW_GetValues($id);" }
